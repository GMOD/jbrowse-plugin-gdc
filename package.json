{
  "name": "jbrowse-plugin-gdc",
  "version": "1.0.3",
  "description": "JBrowse 2 GDC plugin",
  "keywords": [
    "jbrowse",
    "jbrowse2",
    "GDC"
  ],
  "license": "MIT",
  "homepage": "https://github.com/GMOD/jbrowse-plugin-gdc#readme",
  "bugs": "https://github.com/GMOD/jbrowse-plugin-gdc/issues",
  "repository": "github:GMOD/jbrowse-plugin-gdc",
  "author": "Andrew Duncan",
  "main": "dist/index.js",
  "module": "dist/jbrowse-plugin-gdc.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "start": "tsdx watch --verbose --noClean --format umd --name JBrowsePluginGDC --onFirstSuccess \"yarn serve --cors --listen 9000 .\"",
    "prebuild": "npm run clean",
    "build": "tsdx build --format cjs,esm,umd --name JBrowsePluginGDC && cp distconfig.json dist/config.json",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepublishOnly": "yarn test",
    "prepare": "npm run build",
    "postinstall": "jbrowse-plugin-postinstall",
    "postversion": "git push --follow-tags"
  },
  "jbrowse-plugin": {
    "name": "GDC"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "dependencies": {
    "@material-ui/icons": "^4.9.1",
    "uuid": "^7.0.1"
  },
  "peerDependencies": {
    "@jbrowse/core": "^1.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "@jbrowse/core": "^1.2.0",
    "@jbrowse/development-tools": "^1.0.3",
    "@jbrowse/plugin-linear-genome-view": "^1.0.0",
    "@material-ui/core": "^4.9.13",
    "@material-ui/data-grid": "^4.0.0-alpha.19",
    "@material-ui/lab": "^4.0.0-alpha.45",
    "@testing-library/react": "^11.2.5",
    "@types/react": "^17.0.0",
    "mobx": "^5.0.0",
    "mobx-react": "^6.0.0",
    "mobx-state-tree": "3.14.1",
    "prop-types": "^15.0.0",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "regenerator-runtime": "^0.13.7",
    "rimraf": "^3.0.2",
    "rxjs": "^6.0.0",
    "serve": "^11.3.2",
    "tslib": "^2.0.3",
    "typescript": "^4.1.2"
  }
}
