{
  "name": "jbrowse-plugin-gdc",
  "version": "2.1.1",
  "description": "JBrowse 2 GDC plugin",
  "keywords": [
    "jbrowse",
    "jbrowse2",
    "GDC"
  ],
  "license": "MIT",
  "homepage": "https://github.com/GMOD/jbrowse-plugin-gdc#readme",
  "bugs": "https://github.com/GMOD/jbrowse-plugin-gdc/issues",
  "repository": "github:GMOD/jbrowse-plugin-gdc",
  "author": "Andrew Duncan",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "config": {
    "port": 9000,
    "browse": {
      "port": 8999
    },
    "jbrowse": {
      "plugin": {
        "name": "GDC"
      }
    }
  },
  "scripts": {
    "clean": "rimraf dist",
    "proxy": "lcp --proxyUrl https://api.gdc.cancer.gov",
    "start": "npm-run-all --sequential clean --parallel start:*",
    "start:watch": "cross-env JB_NPM=false rollup --config --watch",
    "start:server": "serve --cors --listen $npm_package_config_port .",
    "prebuild": "npm-run-all clean",
    "build": "rollup --config",
    "test": "jest",
    "lint": "eslint --ext .js,.ts,.jsx,.tsx src/",
    "prepublishOnly": "npm-run-all test",
    "prepare": "npm-run-all build",
    "postversion": "git push --follow-tags"
  },
  "jbrowse-plugin": {
    "name": "GDC"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all",
    "arrowParens": "avoid"
  },
  "dependencies": {
    "@material-ui/icons": "^4.11.2",
    "abortable-promise-cache": "^1.4.1",
    "pako": "^2.0.3",
    "quick-lru": "^5.1.1",
    "react-dropzone": "^11.3.2",
    "regenerator-runtime": "^0.13.9",
    "tslib": "^2.3.1",
    "uuid": "^7.0.1"
  },
  "peerDependencies": {
    "@jbrowse/core": "^1.5.2",
    "@jbrowse/plugin-linear-genome-view": "^1.0.0"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.10.4",
    "@jbrowse/cli": "^1.5.1",
    "@jbrowse/core": "^1.5.2",
    "@jbrowse/development-tools": "^1.5.3",
    "@jbrowse/plugin-linear-genome-view": "^1.0.0",
    "@material-ui/core": "^4.12.3",
    "@material-ui/lab": "^4.0.0-alpha.45",
    "@mui/x-data-grid": "^4.0.0-alpha.19",
    "@schemastore/package": "^0.0.6",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@types/jest": "^27.0.3",
    "@types/pako": "^1.0.2",
    "@types/react": "^17.0.0",
    "@types/testing-library__jest-dom": "^5.14.2",
    "@types/testing-library__react": "^10.2.0",
    "@typescript-eslint/eslint-plugin": "^4.0.0",
    "@typescript-eslint/parser": "^4.0.0",
    "babel-eslint": "^10.0.0",
    "cross-env": "^7.0.3",
    "cross-var": "^1.1.0",
    "cypress": "^6.2.1",
    "eslint": "^7.5.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.8",
    "jest": "^27.4.5",
    "jest-fetch-mock": "^3.0.3",
    "local-cors-proxy": "^1.1.0",
    "mobx": "^5.0.0",
    "mobx-react": "^6.0.0",
    "mobx-state-tree": "3.14.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.1",
    "react": "^16.8.0",
    "react-dom": "^17.0.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.59.0",
    "rxjs": "^6.0.0",
    "serve": "^11.3.2",
    "ts-jest": "^27.1.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.1.2"
  }
}
